{
  "name": "pressmedics",
  "version": "0.8.0",
  "flat": true,
  "contributors": [
    "Polymer and Pressmedics Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "hyperpress/pressmedics",
  "scripts": {
    "start": "polymer serve",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && rm -rf server/build && mv build server/ && renamer --find node_modules --replace node_assets 'server/build/**' && replace node_modules node_assets server/build -r -s",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm -l chrome -l firefox",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@material/elevation": "^0.39.0",
    "@material/mwc-button": "^0.2.0",
    "@material/mwc-checkbox": "^0.1.2",
    "@material/radio": "^0.39.1",
    "@material/typography": "^0.39.0",
    "@mdi/font": "^2.6.95",
    "@polymer/app-layout": "^3.0.0-pre.25",
    "@polymer/iron-icon": "^3.0.0-pre.25",
    "@polymer/iron-icons": "^3.0.0-pre.25",
    "@polymer/iron-iconset-svg": "^3.0.0-pre.25",
    "@polymer/lit-element": "dev",
    "@polymer/paper-icon-button": "^3.0.0-pre.25",
    "@webcomponents/webcomponentsjs": "^2.1.1",
    "app-header": "^1.0.9",
    "date-fns": "^2.0.0-alpha.11",
    "fresh": "^0.5.2",
    "growl": "^1.10.5",
    "https-proxy-agent": "^2.2.1",
    "lit-html": "latest",
    "material-design-icons": "^3.0.1",
    "mime": "^2.3.1",
    "moment": "^2.22.2",
    "ms": "^2.1.1",
    "page": "^1.8.6",
    "pwa-helpers": "^0.8.3",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "update": "^0.7.4"
  },
  "devDependencies": {
    "axe-core": "^3.1.1",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.8.0",
    "polyserve": "^0.27.12",
    "prpl-server": "^1.2.0",
    "puppeteer": "^1.7.0",
    "wct-browser-legacy": "^1.0.1"
  }
}
