{
  "name": "pressmedics",
  "version": "0.8.0",
  "flat": true,
  "contributors": [
    "Polymer and Pressmedics Authors"
  ],
  "license": "BSD-3-Clause",
  "repository": "hyperpress/pressmedics",
  "scripts": {
    "start": "polymer serve",
    "build": "npm run build:prpl-server && npm run build:static",
    "build:prpl-server": "polymer build --auto-base-path && rm -rf server/build && mv build server/ && renamer --find node_modules --replace node_assets 'server/build/**' && replace node_modules node_assets server/build -r -s",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm -l chrome -l firefox",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.2",
    "@polymer/iron-icon": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-iconset-svg": "^3.0.1",
    "@polymer/iron-overlay-behavior": "^3.0.2",
    "lit-element": "^2.0.0-rc.2",
    "@polymer/paper-icon-button": "^3.0.1",
    "@polymer/polymer": "^3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.3",
    "date-fns": "^2.0.0-alpha.25",
    "lit-html": "^1.0.0-rc.2",
    "moment-es6": "^1.0.0",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@polymer/test-fixture": "^4.0.2",
    "axe-core": "^3.1.2",
    "chai": "^4.2.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polymer-cli": "^1.9.5",
    "polyserve": "^0.27.15",
    "prpl-server": "^1.4.0",
    "puppeteer": "^1.11.0",
    "wct-mocha": "^1.0.0"
  }
}
